{
  "ai": {
    "provider": "openai",
    "model": "gpt-4",
    "api_endpoint": "https://api.openai.com/v1/chat/completions",
    "max_tokens": 1000,
    "temperature": 0.7,
    "timeout": 30000,
    "retry_attempts": 3,
    "retry_delay": 1000
  },
  "telegram": {
    "api_endpoint": "https://api.telegram.org",
    "message_retention": 1000,
    "update_interval": 5000,
    "max_channels": 50,
    "supported_brokers": [
      "pocket_option",
      "quotex",
      "binomo",
      "iq_option"
    ]
  },
  "analysis": {
    "confidence_threshold": 70,
    "min_historical_signals": 10,
    "max_analysis_time": 10000,
    "signal_keywords": [
      "call", "put", "buy", "sell", "entry", "trade",
      "up", "down", "high", "low", "strike"
    ],
    "timeframes": [
      "1m", "5m", "15m", "30m", "1h", "4h", "1d"
    ],
    "constraints": {
      "martingale": {
        "enabled": true,
        "max_levels": 5,
        "default_multiplier": 2.0
      },
      "timezone": {
        "enabled": true,
        "default": "UTC"
      },
      "expiration": {
        "enabled": true,
        "min_time": 60,
        "max_time": 3600
      }
    }
  },
  "learning": {
    "enabled": true,
    "learning_rate": 0.01,
    "batch_size": 32,
    "update_interval": 3600000,
    "min_confidence_improvement": 0.05
  },
  "performance": {
    "cache_size": 1000,
    "cleanup_interval": 300000,
    "max_concurrent_analysis": 5,
    "memory_limit": "512MB"
  }
}